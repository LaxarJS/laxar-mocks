{
  "name": "laxar-mocks",
  "version": "2.0.0-alpha.0",
  "description": "Building large applications is hard, so don't do that!",
  "homepage": "http://laxarjs.org",
  "main": "laxar-mocks.js",
  "authors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LaxarJS/laxar-mocks.git"
  },
  "scripts": {
    "apidoc": "grunt apidoc",
    "browser-spec": "webpack --config webpack.browser-spec.config.js && webpack-dev-server --port 8087 --inline --config webpack.browser-spec.config.js",
    "eslint": "eslint laxar-mocks.js *.config.js spec",
    "karma": "karma start karma.config.js",
    "optimize": "webpack",
